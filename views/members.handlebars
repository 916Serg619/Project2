    <img src="/assets/Ap4p.png" class="jumbotron" id="memberslogo"></img>

  <main>
    <body>
    <!-- Welcome message -->
    <section class="welcome-user-message">

      <h5>Welcome back, <span class="member-name"></span>!</h5>

    </section>

    <div class="card shadow-lg p-3 mb-5 bg-white rounded" id="members-card">

      <div class="wedding-container newrow">
        <h1>Weddings</h1>
        <table class="table table-striped">
          {{!-- from html-routes --}}
          {{#each event}}
          <tr>
            <td>{{dataValues.couple}}</td>
            <td>{{dataValues.eventDate}}</td>
            {{!-- <td><a href="/summary/{{dataValues.id}}">Summary</a></td> --}}
            <td><button type="button" class="btn navbtn1"
                onclick="window.location = '/summary/{{dataValues.id}}';"><i class="far fa-file"></i> Summary</button>
            </td>
            {{!-- <td><a href="/vendorInfos/{{dataValues.id}}">Add</a></td> --}}
            <td><button type="button" class="btn navbtn1" onclick="window.location = '/guests/{{dataValues.id}}';"><i class="fas fa-user-plus"></i> Add</button></td>
            <td><button type="button" class="btn navbtn1" onclick="window.location = '/vendorInfos/{{dataValues.id}}';"><i class="fas fa-user-plus"></i> Add</button></td>
            {{!-- <td><a style='cursor:pointer;color:red' data-Id="{{dataValues.id}}" class='delete-event'>Delete</a></td> --}}
            <td><button type="button" class="btn navbtn1 delete-event" data-Id="{{dataValues.id}}"><i class="far fa-trash-alt fa-1x"></i> Delete</button>

          </tr>
          {{/each}}
        </table>
        <button type="button" class="btn navbtn1" id="createWedding"><i class="fas fa-heart"></i>
        <br>
        Add Wedding</button>
      </div>
    </div>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script type="text/javascript" src="js/members.js"></script>
    <script src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/16327/gsap-latest-beta.min.js"></script>

  </body>
</main>