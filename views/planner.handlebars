<body>
    <main>
        <div class="card">
            <div class="card-header">
                <h3>Add Wedding Information</h3>
            </div>
            <form class="planner">
                {{!-- Couple --}}
                <div class="form-group">
                    <label for="couple">Couple</label>
                    <input type="text" class="form-control" id="coupleInput" placeholder="Couple">
                </div>
                {{!-- Venue --}}
                <div class="form-group">
                    <label for="venue">Venue</label>
                    <input type="text" class="form-control" id="venueInput" placeholder="Venue">
                </div>
                {{!-- Event Date --}}
                <div class="form-group">
                    <label for="date">Date</label>
                    <input type="date" class="form-control" id="dateInput">
                </div>
            </form>
            <button type="button" class="btn navbtn1" id="submitNewWedding">Submit<br><i
                    class="fas fa-sign-in-alt"></i></button>
        </div>
        {{!-- Navigation buttons --}}

        {{!-- <button type="button" class="btn btn-success" id="backToMain">Main</button> --}}
        {{!-- <button type="button" class="btn btn-success" id="toSummary">Summary</button> --}}
        </div>
        {{!-- Scripts --}}
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
        {{!-- API key for Google API autocomplete feature for Venue info --}}
        <script type="text/javascript"
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDZugWOlMVS7_70ajiCk05Mcn_g7qHMsls&libraries=places"></script>
        <script type="text/javascript" src="js/planner.js"></script>
        <script src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/16327/gsap-latest-beta.min.js"></script>


    </main>
</body>