            {{!-- Venue --}}
            <div class="form-group">
                <label for="venue">Venue</label>
                <input type="text" class="form-control" id="venueInput" placeholder="Venue">
            </div>
            {{!-- Event Date --}}
            <div class="form-group">
                <label for="date">Date</label>
                <input type="date" class="form-control" id="dateInput">
            </div>
        </form>

        {{!-- Navigation buttons --}}
        <button type="button" class="btn navbtn1" id="submitNewWedding">Submit<br><i class="fas fa-sign-in-alt"></i></button>
